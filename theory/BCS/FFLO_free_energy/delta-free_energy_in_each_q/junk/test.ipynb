    "from numpy import *\n",
    "N=10\n",
    "y = -1 + 2 * arange(2)\n",
    "print(y)\n",
    "k1 = -1 + 2 * arange(N)  / (N)\n",
    "kx, ky, y = meshgrid(k1, k1, y, indexing='ij')\n",
    "print(\"kxは\", shape(kx))\n",
    "print(\"yは\", shape(y))\n",
    "print(type( exp(kx + ky + y)))\n",
    "f = exp(kx + ky + y)\n",
    "print(type(sum(f)))\n"
